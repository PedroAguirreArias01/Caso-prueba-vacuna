<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "//Hibernet/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.DosisAplacad" table="dosis_aplicada">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="fechaAplicacion" type="java.time.LocalDate">
            <column name="fecha_aplicacion" not-null="true"/>
        </property>
        <many-to-one column="id_historial" name="historial"/>
        <many-to-many column="id_dosis" name="dosis"/>
    </class>
</hibernate-mapping>
