<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "//Hibernet/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Familiar" table="FAMILIARES">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="personaPadre" column="id_padre"/>
        <many-to-one name="personaHijo" column="id_hijo"/>
    </class>
</hibernate-mapping>