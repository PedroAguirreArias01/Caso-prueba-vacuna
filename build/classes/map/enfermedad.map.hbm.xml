<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "//Hibernet/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Enfermedad" table="ENFERMEDADES">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="nombre" type="string">
            <column name="nombre"/>
        </property>
        <property name="descripcion" type="string">
            <column name="descripcion"/>
        </property>
        <set name="listaVacuna" table="ENFERMEDAD_VACUNAS" cascade="all" inverse="true">
            <key column="id_enfermedad"/>
            <many-to-many column="id_vacuna" class="models.Vacuna"/>
        </set>
    </class>
</hibernate-mapping>