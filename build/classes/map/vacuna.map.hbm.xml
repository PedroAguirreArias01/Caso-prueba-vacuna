<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "//Hibernet/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Vacuna" table="VACUNAS">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="nombre" type="string">
            <column name="nombre"/>
        </property>
        <property name="descripcion" type="string">
            <column name="descripcion"/>
        </property>
        <set name="listEnfermedades" table="ENFERMEDAD_VACUNAS" cascade="all" inverse="false">
            <key column="id_vacuna"/>
            <many-to-many column="id_enfermedad" class="models.Enfermedad"/>
        </set>
    </class>
</hibernate-mapping>